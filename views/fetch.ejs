<%- include('header') %>

    <link rel='stylesheet' href='ㄥ' />

<h2>確診人數細項資料 </h2>

<!-- 分頁導覽列 -->
<ul class="pagination">
  <li>第 <%=items.pageNo%> 頁</li>

  <% if(items.startPage>1){ %>
  <li><a href="/fetch/<%=(items.startPage-items.navSegments)%>">...</a></li>
  <% } %>

  <% for(var i=items.startPage; i< (items.startPage+items.navSegments); i++) {%>
  <% if(i<=items.totalPage){ %>
  <li><a href="/fetch/<%=i%>"><%=i%></a></li>
  <% } %>
  <% } %>

  <% if(items.startPage+items.navSegments<=items.totalPage){ %>
  <li><a href="/fetch/<%=(items.startPage+items.navSegments)%>">...</a></li>
  <% } %>
</ul>

<!-- 顯示資料表格 -->
<table>
  <thead>
    <tr>
      <th>編號</th>
      <th>日期</th>
      <th>城市</th>
      <th>位置</th>
      <th>性別</th>
      <th>本地</th>
      <th>年紀</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i<items.data.length; i++) {%>
    <tr>
      <td><a href="/fetch/<%= items.data[i].proNo %>"><%= items.data[i].id %></a></td>
      <td><%= items.data[i].date %></td>
      <td><%= items.data[i].city %></td>
      <td><%= items.data[i].location %></td>
      <td><%= items.data[i].gender %></td>
      <td><%= items.data[i].locally %></td>
      <td><%= items.data[i].age %></td>
    </tr>
    <% } %>
  </tbody>
</table>

<%- include('footer') %>